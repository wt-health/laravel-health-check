{
    "name": "wthealth/laravel-health-check",
    "description": "Check versions of vital parts of the application",
    "require": {
        "spatie/laravel-package-tools": "^1.16.4",
        "composer/composer": "^2",
        "spatie/laravel-health": "^1.27",
        "php": "^8.2|^8.3",
        "illuminate/support": "^9.0|^10.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Webtools\\LaravelHealthCheck\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Webtools\\LaravelHealthCheck\\LaravelHealthCheckServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fernando Guedes",
            "email": "fernando@webtools.co.nz"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require-dev": {
        "webtoolshealth/php-coding-standard": "^1.0"
    },
    "scripts": {
        "ecs": "vendor/bin/ecs check",
        "ecs-fix": "vendor/bin/ecs check --fix"
    }
}
